{
  "ci": {
    "collect": {
      "staticDistDir": ".",
      "url": [
        "/index.html",
        "/guia.html",
        "/corredores.html"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "assertionLevel": "warn",
      "assertions": {
        "legacy-javascript": "warn",
        "render-blocking-insight": "off",
        "uses-long-cache-ttl": "off",
        "speed-index": ["warn", { "minScore": 0.05 }],
        "largest-contentful-paint": "warn",
        "mainthread-work-breakdown": "warn"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}